<div id="<%= dom_id shift %>">
  <p>
    <strong>Name:</strong>
    <%= shift.name %>
  </p>

  <p>
    <strong>Working hours:</strong>
    <%= shift.working_hours %>
  </p>

  <p>
    <strong>Start time:</strong>
    <%= shift.start_time %>
  </p>

  <p>
    <strong>End time:</strong>
    <%= shift.end_time %>
  </p>

  <p>
    <strong>Working days:</strong>
    <%= shift.working_days %>
  </p>

</div>
