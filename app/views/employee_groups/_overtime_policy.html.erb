<%= form.fields_for :time_policy do |ff| %>
  <%= ff.fields :overtime_policy, model: OpenStruct.new(form.object.time_policy.overtime_policy) do |overtime_policy| %>
    <div class="mt-3">
      <p>Overtime Policy</p>
      <%= overtime_policy.radio_button :overtime, false, checked: true %>
      <%= overtime_policy.label :overtime, "No", value: "no"%><br>
    </div>
    <div class="text-center mt-2 mb-2">
      <%= link_to 'Back','#',data: {controller: "time-policy-rules", action: "click->time-policy-rules#missingSwipe"} %>
    </div>
  <% end %>
<% end %>
